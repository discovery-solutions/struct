{
    "name": "@discovery-solutions/struct",
    "version": "1.0.0",
    "type": "module",
    "description": "Framework para criação estruturada de forms e CRUDs com Next.js, React Hook Form, Zod e ShadCN.",
    "author": "Lucas Paes <lucas@dscvr.solutions>",
    "scripts": {
        "dev": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json --watch",
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "lint": "eslint . --ext .ts,.tsx",
        "test": "vitest"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        },
        "./client": {
            "import": "./dist/esm/client/index.js",
            "require": "./dist/cjs/client/index.js",
            "types": "./dist/esm/client/index.d.ts"
        }
    },
    "peerDependencies": {
        "@tanstack/react-table": "*",
        "@tanstack/react-query": "*",
        "aws4": "*",
        "class-variance-authority": "*",
        "lucide-react": "*",
        "mongoose": "*",
        "next": "15.*",
        "react": "19.*",
        "react-dom": "19.*",
        "tailwind-merge": "*",
        "tailwindcss": "*",
        "zod": "*"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^19",
        "eslint": "^9",
        "prettier": "^3.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5",
        "vitest": "^3.2.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/discovery-solutions/struct.git"
    },
    "bugs": {
        "url": "https://github.com/discovery-solutions/struct/issues"
    },
    "license": "MIT",
    "keywords": [],
    "homepage": "https://github.com/discovery-solutions/struct#readme",
    "files": [
        "dist",
        "src"
    ]
}
